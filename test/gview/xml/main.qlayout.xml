<hui version="1.0">

 <!-- This definition support based QWidget layout OR HGWidget
   if parent is QWidget,then all layout items are based QWidget object
   if parent is HGWidget,then layout items are  based QWidget Or based QGrahicsWidget
 <!-->
 <layout>

  <widget class="QPushButton" name="" styleid="undefined">
          <property id="text">text0</property>
          <property id="icon"></property>
          <property id="proxy:fixedHeight">60</property>
          <property id="proxy:fixedWidth">100</property>
          <!-- 120:true|4:false|78:true
               120=Qt::WA_TranslucentBackground
               4=Qt::WA_OpaquePaintEvent
               78=Qt::WA_AcceptDrops
          <!-->
          <property id="proxy:winowAttribute"></property>
          <ltconf row="0" col="0" alignment="Qt::AlignRight"/>
  </widget>

  <widget class="QPushButton" name="" styleid="undefined">
          <property id="text">text1</property>
          <property id="icon"></property>
          <property id="proxy:fixedHeight">48</property>
          <ltconf row="1" col="0" />
  </widget>

  <widget class="QPushButton" name="" styleid="undefined">
          <property id="text">text2</property>
          <property id="icon"></property>
          <ltconf row="2" col="0" stretch="2"/>
  </widget>

  <widget class="QRadioButton" name="" styleid="undefined">
          <property id="text">text1</property>
          <property id="icon"></property>
          <property id="checked">1</property>
          <ltconf row="3" col="0" />
  </widget>

  <widget class="QLineEdit" name="" styleid="undefined">
          <property id="text">I am a QLineEdit</property>
          <property id="icon"></property>
          <property id="proxy:fixedHeight">32</property>
          <!--property id="echoMode">QLineEdit::Password</property><!-->
          <ltconf row="4" col="0" />
  </widget>

</layout>

</hui>

