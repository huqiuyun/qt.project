<?xml version="1.0" encoding="UTF-8"?>
<hui version="1.0">

 <!-- support based QWidget layout OR HGWidget
   if parent is QWidget,then all layout items are based QWidget object
   if parent is HGWidget,then layout items are  based QWidget Or based QGrahicsWidget
 <!-->
 <layout>

   <!-- Style is private , belong to all childs.
        If child need to use them , then need to add the key <style/>.
   <!-->
   <style>
     <obj class="HBackgroundStyle" name="" styleid="backgroundId2">
       <property id="sysTile">true</property>
       <property id="imagePath">:/theme/bg.png</property>
     </obj>

     <obj class="HGWidgetStyle" name="" styleid="gwidgetId2">
       <obj id="backgroundStyle" class="HBackgroundStyle" name="backgroundStyle" styleid="backgroundId2"/>
     </obj>

    <obj class="HGLayoutStyle" name="" styleid="glayoutId3">
       <property id="layoutType">kHBox</property>
       <property id="alignment"></property>
       <property id="margins">0,0,0,0</property>
       <property id="spacing">6</property>
    </obj>

     <obj class="HGWidgetStyle" name="" styleid="gwidgetId3">
       <property id="styleSheet"></property>
       <obj id="layoutStyle" class="HGLayoutStyle" name="layoutStyle" styleid="glayoutId3"/>
     </obj>

     <obj class="HQLayoutStyle" name="" styleid="qlayoutId3">
        <property id="layoutType">kHBox</property>
        <property id="alignment"></property>
        <property id="margins"></property>
        <property id="spacing">1</property>
     </obj>

     <obj class="HFrameStyle" name="" styleid="gwidgetframeId">
        <property id="windowFlags">0</property>
        <property id="styleSheet"></property>
        <obj id="layoutStyle" class="HQLayoutStyle" name="layoutStyle" styleid="qlayoutId3"/>
     </obj>

   </style>

   <widget class="HGWidget" name="" styleid="gwidgetId3">
      <style/>
      <property id="color"></property>
      <property id="openGraphicsItemFlag">16</property>
      <layout>
        <widget class="HQWidget" name="" styleid="gwidgetframeId">
          <style/>
          <property id="proxy:noSystemBackground">1</property>
          <layout file=":xml/main.qlayout.xml" />
        </widget>
      </layout>
      <property id="skip:alignment">Qt::AlignCenter</property>
   </widget>

   <widget class="HGWidget" name="" styleid="gwidgetId4">
      <style>
       <obj class="HGWidgetStyle" name="" styleid="gwidgetId4">
          <obj id="backgroundStyle" class="HBackgroundStyle" name="backgroundStyle" styleid="backgroundId2"/>
          <obj id="layoutStyle" class="HGLayoutStyle" name="" styleid="childid"/>
      </obj>
      </style>
      <property id="color">100,40,200</property>
      <widget class="HGWidget" name="" styleid="gwidgetId2">
         <style/>
         <property id="color">140,140,140</property>
         <ltconf margins="4,10,-4,-10"/>
      </widget>
   </widget>

   <widget class="HGWidget" name="" styleid="gwidgetId2">
     <style/>
     <property id="color">101,101,101</property>
   </widget>

   <widget class="HGWidget" name="" styleid="gwidgetId3">
     <style/>
      <layout>
        <widget class="HQWidget" name="" styleid="private.framewindowId">
          <style/>
          <property id="margins">10,10,10,10</property>
          <property id="visible">true</property>
          <layout file=":xml/main.qlayout.xml" />
          <property id="proxy:noSystemBackground">1</property>
          <property id="skip:alignment">Qt::AlignLeft</property>
        </widget>

        <widget class="HQWidget" name="" styleid="private.framewindowId">
          <style/>
          <property id="margins">10,10,10,10</property>
          <layout file=":xml/main.qlayout.xml" />
          <property id="skip:alignment">Qt::AlignLeft</property>
        </widget>

        <widget class="HQWidget" name="" styleid="private.framewindowId">
          <style/>
          <property id="margins">10,10,10,10</property>
          <layout file=":xml/main.qlayout.xml" />
          <property id="skip:alignment">Qt::AlignLeft</property>
        </widget>
     </layout>
  </widget>
  <widget class="HGStackedWidget" name="" styleid="gwidgetId2">
    <style/>
    <property id="color">0,255,0</property>
    <property id="fixedHeight">60</property>
    <layout>
       <widget class="HGWidget" name="" styleid="gwidgetId2">
          <style/>
          <property id="color">255,0,0</property>
       </widget>
       <widget class="HGWidget" name="" styleid="gwidgetId2">
          <style/>
          <property id="color">0,255,0</property>
       </widget>
    </layout>
    <property id="currentIndex">0</property>
  </widget>

</layout>

</hui>
