 <!-- css for public <!-->
 <style>

    <obj class="HFontStyle" name="" styleid="fontId">
       <property id="family">雅黑</property>
       <property id="pointSize"></property>
       <property id="pointSizeF"></property>
       <property id="pixelSize">40</property>
       <property id="weight"></property>
       <property id="bold">true</property>
       <property id="italic">true</property>
       <property id="underline">true</property>
       <property id="overline"></property>
       <property id="strikeOut"></property>
       <property id="fixedPitch"></property>
       <property id="kerning"></property>
       <property id="stretch"></property>
       <property id="styleStrategy"></property>
       <property id="wordSpacing"></property>
       <property id="capitalization"></property>
       <property id="stretch"></property>
       <property id="hintingPreference"></property>
       <property id="rawMode"></property>
       <property id="rawName"></property>
    </obj>

    <obj class="HColorStyle" name="" styleid="colorId">
       <invoke method="setColor">
         <arg type="int">0</arg>
         <arg type="color">200,0,200</arg>
       </invoke>
       <invoke method="setColor">
         <arg type="int">1</arg>
         <arg type="color">200,200,200</arg>
       </invoke>
    </obj>

