<?xml version="1.0" encoding="UTF-8"?>
<hui version="1.0">

<!--
    1. This definition is only parent and child .

    2. If parent is QWidget , then child only is based QWidget.
       @exmaple, create failure:
       <widget class="HQWidget" styleid="undefined">
         <widget class="HGWidget" name="" styleid="gwidgetId"/>
       </widget>

    3. If parent is QGraphicWidget,then child only is based GraphicsWidget.
     @exmaple,create failure:
     <widget class="HGWidget" styleid="undefined">
       <widget class="HQWidget" name="" styleid="gwidgetId"/>
     </widget>

     @exmaple, create success, it can be in the <layout>, but parent need be layout style:
     <widget class="HGWidget" name="" styleid="gwidgetId">
        <style>
            @see main.style.xml , HGWidgetStyle
        </style>
        <layout>
          <widget class="HQWidget" name="" styleid="undefined"/>
        </layout>
     </widget>
<!-->

 <widget class="HGView" name="main" styleid="my.framewindow.test1">
   <!-- style is private <!-->
   <style>
      <obj class="HFrameStyle" name="" styleid="my.framewindow.test1">
         <property id="windowFlags">Qt::Window</property>
         <property id="styleSheet"></property>
         <property id="layoutStyle">
            <class class="HQLayoutStyle" name="layoutStyle" styleid="my.qvbox.layoutId"/>
         </property>
         <property id="sceneStyle">
            <class class="HGSceneStyle" name="sceneStyle" styleid="sceneId"/>
         </property>
       </obj>
       <obj class="HQLayoutStyle" name="" styleid="my.qvbox.layoutId">
          <property id="layoutType">kVBox</property>
          <property id="alignment"></property>
          <property id="margins">4,4,4,4</property>
          <property id="spacing">6</property>
       </obj>
   </style>
   <property id="resize">400,600</property>
   <property id="windowTitle">test1-hgview</property>
   <scene main="true">
     <widget class="HGWidget" name="" styleid="gwidgetId">
        <layout>
           <widget class="QTextEdit" name=""  styleid="undefined">
             <property id="plainText">I am QTextEdit</property>
           </widget>
           <widget class="QTextBrowser" name="" styleid="undefined">
             <property id="plainText">I am QTextBrowser></property>
             <property id="readOnly">0</property>
           </widget>
        </layout>
     </widget>
   </scene>
 </widget>

 <widget class="HQWidget" name="main" styleid="my.framewindow.test2">
   <style>
     <obj class="HFrameStyle" name="" styleid="my.framewindow.test2">
       <property id="windowFlags">Qt::Window</property>
       <property id="styleSheet"></property>
       <property id="layoutStyle">
          <class class="HQLayoutStyle" name="layoutStyle" styleid="my.qgrid.layoutId"/>
       </property>
    </obj>
    <obj class="HQLayoutStyle" name="" styleid="my.qgrid.layoutId">
       <property id="layoutType">kGrid</property>
       <property id="alignment"></property>
       <property id="margins">4,4,4,4</property>
       <property id="spacing">6</property>
    </obj>
   </style>

   <property id="resize">600,400</property>
   <property id="windowTitle">test2-hqwidget</property>
   <layout file=":/xml/main.qlayout.xml"/>
 </widget>

</hui>
